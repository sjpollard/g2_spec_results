#!/bin/bash
#SBATCH --job-name SPEC_tiny_gnu9_256_2
#SBATCH --partition graviton2
#SBATCH --nodes 8
#SBATCH --ntasks 256
#SBATCH --ntasks-per-node 32
#SBATCH --cpus-per-task 2
#SBATCH --time 03:00:00
#SBATCH --output SPEC_tiny_gnu9_256_2.out

cd ../../../../../lustre/SPEChpc
. ./shrc
cd $SLURM_SUBMIT_DIR

runhpc --rebuild --define model=omp --define ppn=32 --pmodel=OMP --config=aws_g2_gnu9.cfg --ranks=256 --threads=2 --reportable --output_format=all --output_root=/home/sam/g2_spec_results/tiny/tiny_gnu9_256_2 --iterations=2 tiny
