#!/bin/bash
#SBATCH --job-name SPEC_tiny_gnu9_64
#SBATCH --partition graviton2
#SBATCH --nodes 1
#SBATCH --ntasks 64
#SBATCH --time 08:00:00
#SBATCH --output SPEC_tiny_gnu9_64.out

cd ../../../../../lustre/SPEChpc
. ./shrc
cd $SLURM_SUBMIT_DIR

spack load gcc@9.3.0
spack load openmpi@4.1.1 %gcc@9.3.0

runhpc --rebuild --config=aws_g2_gnu9.cfg --ranks=64 --noreportable --size=test --output_format=all --output_root=/home/sam/g2_spec_results/tiny/tiny_gnu9_64 --iterations=2 tiny
