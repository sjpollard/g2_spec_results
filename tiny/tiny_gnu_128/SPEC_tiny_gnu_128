#!/bin/bash
#SBATCH --job-name SPEC_tiny_gnu_128
#SBATCH --partition graviton2
#SBATCH --nodes 2
#SBATCH --ntasks 128
#SBATCH --time 06:00:00
#SBATCH --output SPEC_tiny_gnu_128.out

cd ../../../../../lustre/SPEChpc
. ./shrc
cd $SLURM_SUBMIT_DIR

runhpc --rebuild --config=aws_g2.cfg --ranks=128 --reportable --output_format=all --output_root=/home/sam/g2_spec_results/tiny/tiny_gnu_128 --iterations=2 tiny
