#!/bin/bash
#SBATCH --job-name SPEC_tiny_gnu_64
#SBATCH --partition graviton2
#SBATCH --nodes 1
#SBATCH --ntasks 64
#SBATCH --time 04:00:00
#SBATCH --output SPEC_tiny_gnu_64.out

cd ../../../../../lustre/SPEChpc
. ./shrc
cd $SLURM_SUBMIT_DIR

runhpc --rebuild --config=aws_g2.cfg --ranks=64 --reportable --output_format=all --output_root=/home/sam/g2_spec_results/tiny/tiny_gnu_64 --iterations=2 tiny
