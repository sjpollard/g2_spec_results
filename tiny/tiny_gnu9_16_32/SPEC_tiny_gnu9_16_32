#!/bin/bash
#SBATCH --job-name SPEC_tiny_gnu9_16_32
#SBATCH --partition graviton2
#SBATCH --nodes 8
#SBATCH --ntasks 16
#SBATCH --ntasks-per-node 2
#SBATCH --cpus-per-task 32
#SBATCH --time 03:00:00
#SBATCH --output SPEC_tiny_gnu9_16_32.out

cd ../../../../../lustre/SPEChpc
. ./shrc
cd $SLURM_SUBMIT_DIR

runhpc --rebuild --define model=omp --define ppn=2 --pmodel=OMP --config=aws_g2_gnu9.cfg --ranks=16 --threads=32 --reportable --output_format=all --output_root=/home/sam/g2_spec_results/tiny/tiny_gnu9_16_32 --iterations=2 tiny
