#!/bin/bash
#SBATCH --job-name SPEC_tiny_gnu9_8_64
#SBATCH --partition graviton2
#SBATCH --nodes 8
#SBATCH --ntasks 8
#SBATCH --ntasks-per-node 1
#SBATCH --cpus-per-task 64
#SBATCH --time 03:00:00
#SBATCH --output SPEC_tiny_gnu9_8_64.out

cd ../../../../../lustre/SPEChpc
. ./shrc
cd $SLURM_SUBMIT_DIR

runhpc --rebuild --define model=omp --define ppn=1 --pmodel=OMP --config=aws_g2_gnu9.cfg --ranks=4 --threads=64 --reportable --output_format=all --output_root=/home/sam/g2_spec_results/tiny/tiny_gnu9_8_64 --iterations=2 tiny
